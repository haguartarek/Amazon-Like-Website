@model WebApplication2.ViewModels.RegisterUserViewModel




<form id="ResetPasswordForm" method="post" asp-action="SaveResetSellerPassword" asp-controller="Admin" onsubmit="event.preventDefault()">
    <div asp-validation-summary="All"></div>
    <div>
       
            <div class="form-group">
                <label asp-for="Password" class="control-label"></label>
                <input asp-for="Password" class="form-control" />
                <span asp-validation-for="Password" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="ConfirmPassword" class="control-label"></label>
                <input asp-for="ConfirmPassword" class="form-control" />
                <span asp-validation-for="ConfirmPassword" class="text-danger"></span>
            </div>

       
    </div>
    <button id="ResetPassword" type="submit" onclick="submitForm2()" class="btn prim-bg prim-btn">Reset</button>
    

    </div>
</form>

